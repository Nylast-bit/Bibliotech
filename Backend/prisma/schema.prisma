generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}


datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Libro {
  ID      Int      @id @default(autoincrement())
  ISBN   Int   @unique
  TITULO    String
  AUTOR   String 
  EDITORIAL String
  YEAR_EDICION Int
  GENERO String
  CALIFICACION Decimal
  FECHA_CREACION DateTime @default(now())
}


